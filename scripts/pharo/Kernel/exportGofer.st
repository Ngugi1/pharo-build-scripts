#(   'Network-Kernel'    'Network-MIME'    'Network-UUID'     'Network-Url'     'Network-URI'     'Network-Protocols'   'PackageInfo'     'System-FileRegistry'     'Monticello'     'MonticelloConfigurations'     'Zinc-HTTP'   'Zinc-Patch-HTTPSocket'   'Settings-Network'   'Settings-Monticello'   'Gofer-Core') do: [:packageName |	((SmalltalkImage current getSystemAttribute: 3) ifNil: [FileDirectory default] ifNotNilDo: [:path | FileDirectory on: path]) forceNewFileNamed: (packageName, '.st') do: [:file |		file nextPutAll: (ScriptLoader latestSource: packageName location: 'http://www.squeaksource.com/Pharo')]].!SmalltalkImage current snapshot: false andQuit: true.